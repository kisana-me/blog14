<table>
  <thead>
    <tr>
      <% instead.columns.each do |column| %>
        <th><%= column.name.titleize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% instead.each do |i| %>
      <tr>
        <% instead.columns.each do |column| %>
          <td><%= i.send(column.name) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
