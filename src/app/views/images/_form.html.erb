<%= form_with model: @image, url: url, data: { turbo: false } do |f| %>
  <%= render 'shared/error_messages', instead: @image %>
  <% if new %>
  <div>
    <%= f.label :image, 'image' %>
    <%= f.file_field :image %>
  </div>
  <% end %>
  <div>
    <%= f.label :name, '名前' %>
    <%= f.text_field :name, placeholder: '名前' %>
  </div>
  <br/>
  <div>
    <%= f.label :public_visibility, '公開' %>
    <%= f.check_box :public_visibility ,{}, "1", "0" %>
  </div>
  <br/>
  <%= f.submit 'アップロード' %>
<% end %>